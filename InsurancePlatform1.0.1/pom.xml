<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>InsurancePlatform1.0.1</artifactId>
  <packaging>war</packaging>
  <name>InsurancePlatform Maven Webapp</name>
  <url>http://maven.apache.org</url>
   <build>
    <finalName>InsurancePlatform</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<!--tomcat发布端口号  -->
					<port>8091</port>
					<!--项目访问的根目录 localhost:8091/-->
					<path>/</path>
				</configuration>
			</plugin>
		</plugins>
</build>
<dependencies>
  <dependency>
  		<groupId>com.zlb</groupId>
  		<artifactId>InsurancePlatform-common</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
  	</dependency>
  </dependencies>
 
  
	<parent>
		<groupId>com.zlb</groupId>
		<artifactId>InsurancePlatform-parent</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
</project>
